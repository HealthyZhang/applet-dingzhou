page{
  height: 100%;
}
.music{
  position: fixed;
  top: 50rpx;
  right: 50rpx;
  width: 50rpx;
  height: 50rpx;
  padding: 4rpx;
  border-radius: 50%;
  z-index: 99;
  background: rgba(150,189,223,.3);
}
.arrow{
  opacity: 0;
  position: fixed;
  bottom: 50rpx;
  left: 0;
  right: 0;
  margin: auto;
  width: 150rpx;
  height: 50rpx;
  z-index: 99;
}
.page_up{
  position: absolute;
  width: 100%;
  height: 100%;
  top: -100%;
  left: 0;
}
.page_down{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 100%;
  left: 0;
}
.page_now{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
._20index{
  z-index: 20;
}
._10index{
  z-index: 10;
}