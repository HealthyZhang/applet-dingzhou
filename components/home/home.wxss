@keyframes rota{
    from{}
    to{transform: rotateY(720deg) rotateX(30deg);}
}
.star-home{
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
}
.star-home .bg{
    /* display: block; */
    width: 100%;
    height: 100%;
}
.star{
    opacity: 0;
    width: 100rpx;
    height: 100rpx;
    border: solid 1rpx #FFF;
    border-radius: 50%;
    position: absolute;
    top: -100rpx;
    left: -100rpx;
    /* background-image: linear-gradient(to right,rgba(255,255,255,.1),rgba(255,255,255,.7)); */
    /* transform: rotate(63.3deg); */
}
.pics{
    position: absolute;
    width:150rpx;
    height:200rpx;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    margin: auto;
    transform-style: preserve-3d;
    animation: rota 5s infinite alternate;
    animation-delay: 1s;
}
.pics2{
    position: absolute;
