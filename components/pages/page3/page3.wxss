.page3{
    /* background: #333; */
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
}
.page3 .bg{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 10;
}
/* title */
.page3 .title{
    position: relative;
    z-index: 11;
    text-align: center;
}
.page3 .title image{
    width: 400rpx;
    height: 0;
    display: block;
    margin: 0 auto;
}
.page3 .title text{
    color: #FFF;
    font-size:50rpx;
    opacity: 0;
}
.ta{
    position: absolute;
    z-index: 11;
    width: 420rpx;
    height: 560rpx;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
}