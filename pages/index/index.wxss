@keyframes rotate{
  from{transform: rotate(0deg)}
  to{transform: rotate(359deg)}
}
.song{
  animation: rotate 1.2s linear infinite;
}
.dingzhou{
  height: 100%;
  box-sizing: border-box;
  background: #000;
}
/*page1*/
.page1{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.page1 .one {
  width: 150rpx;
  height: 150rpx;
  position: absolute;
  z-index: 2;
  opacity: 0;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  transform: translate(-100%) rotate(-90deg);
}
.page1 .two{
  width: 300rpx;
  height: 300rpx;
  position: absolute;
  z-index: 3;
  opacity: 0;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  transform: translate(-100%) rotate(-90deg);
}
.page1 .three{
  width: 450rpx;
  height: 450rpx;
  position: absolute;
  z-index: 4;
  opacity: 0;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  transform: translate(-100%) rotate(-90deg);

}
.page1 .four{
  width: 600rpx;
  height: 600rpx;
  position: absolute;
  z-index: 5;
  opacity: 0;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  transform: translate(-100%) rotate(-90deg);

}
.page1 .five{
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: 6;
  opacity: 0;
  transform: translate(-100%) rotate(-90deg);
}
.out{
  width: 410rpx;
  height: 310rpx;
  border: 1rpx solid #ddb45f;
  margin: auto;
  margin-top: 100rpx;
  padding-top: 10rpx;
  box-sizing: border-box;
  background: rgba(0,0,0,.3);
  opacity: 0;
  position: relative;
  z-index: 6;
  transform: translate(-100%) rotate(-90deg);
}
.inner{
  opacity: 0;
  position: absolute;
  top: 10rpx;
  left: 10rpx;
  width: 390rpx;
  height: 290rpx;
  border: 1rpx solid #ddb45f;
  box-sizing: border-box;
  z-index: 6;
  transform: translate(-100%) rotate(-90deg);
}
.inner image{
  width: 390rpx;
  height: 290rpx;
  height: 0;
  opacity: 0;
}
/*box*/
.box{
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: absolute;
  top: 0;
}
/*page*/
.page1{
  position: absolute;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
}
.page2{
  position: absolute;
  top: 100%;
  left: 0;
  height: 100%;
  width: 100%;
}
.page3{
  position: absolute;
  top: 200%;
  left: 0;
  height: 100%;
  width: 100%;
}